{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' http://localhost:8000 https://mecai.app https://novelvisionai.art;"
        }
      ]
    }
  ]
}
